{% extends '@AcMarchePivot/layout.html.twig' %}

{% block body %}

    <div class="card">
        <h3 class="card-header">{{ offres|length }} offre(s)</h3>
        <div class="card-body">

            <table class="table table-bordered">
                <thead>
                <tr>
                    <th>Nom</th>
                    <th>X</th>
                    <th>Y</th>
                </tr>
                </thead>
                <tbody>
                {% for offre in offres %}
                    <tr>
                        <td>{{ offre.nom }}</td>
                        <td></td>
                        <td></td>
                    </tr>
                {% endfor %}
                </tbody>
            </table>
        </div>
    </div>
{% endblock %}
